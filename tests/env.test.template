# Test Environment Variables Template
# Copy this file to .env.test and fill in your actual test values

# Test User Credentials
TEST_USER_PASSWORD=your_test_user_password_here

# Test Database Configuration
TEST_DB_HOST=localhost
TEST_DB_PORT=5433
TEST_DB_NAME=test_database
TEST_DB_USER=test_user
TEST_DB_PASSWORD=your_test_db_password_here

# Test JWT Secret
TEST_JWT_SECRET=your_test_jwt_secret_here

# Debug Environment
DEBUG_DB_HOST=database
DEBUG_DB_PORT=5432
DEBUG_DB_NAME=debug_database
DEBUG_DB_USER=debug_user
DEBUG_DB_PASSWORD=your_debug_db_password_here

# Production Environment (for testing production configs)
PROD_DB_HOST=production_host
PROD_DB_PORT=5432
PROD_DB_NAME=production_database
PROD_DB_USER=production_user
PROD_DB_PASSWORD=your_production_db_password_here
PROD_JWT_SECRET=your_production_jwt_secret_here
