{
  "name": "coolanu",
  "version": "1.0.0",
  "description": "Coolanu (table-surfing) alpha",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "export NODE_ENV=production && node server.js",
    "server": "export NODE_ENV=production && nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "export NODE_ENV=production && npm run-script client-install && cd client && react-scripts build; cd ..",
    "stop": "node server.stop.js",
    "start-db": "docker-compose -p coolanu-db -f db/local-docker-compose.yaml up -d ",
    "stop-db": "docker-compose -p coolanu-db -f db/local-docker-compose.yaml down",
    "migrate-local-db": "bash ./db/migrate.sh",
    "debug": "concurrently \"nodemon server.js\" \"npm run client\""
  },
  "author": "Mark&Yana",
  "license": "MIT",
  "dependencies": {
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "DatePicker": "^2.0.0",
    "aws-sdk": "^2.683.0",
    "bcryptjs": "^2.4.3",
    "bluebird": "^3.7.2",
    "body-parser": "^1.18.3",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.11.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "file-type": "^14.5.0",
    "firebase": "^7.15.5",
    "fs": "0.0.1-security",
    "helmet": "^3.22.0",
    "heroku-ssl-redirect": "0.0.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "multiparty": "^4.2.1",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "postgres": "^1.0.2",
    "react-datepicker": "^2.14.1",
    "react-helmet": "^5.2.1",
    "react-scripts": "^3.4.3",
    "react-time-picker": "^3.9.0",
    "validator": "^12.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/komapc/MERN"
  },
  "engines": {
    "node": "12.18.2"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
