{
  "name": "coolanu",
  "version": "1.0.0",
  "description": "Coolanu (table-surfing) alpha",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run-script client-install && cd client && react-scripts build; cd ..",
    "stop": "node server.stop.js",
    "start-db": "docker-compose -p coolanu -f db/local-docker-compose.yaml up -d",
    "stop-db": "docker-compose -p coolanu -f db/local-docker-compose.yaml down",
    "migrate-local-db": "chmod +x db/migrate.sh && db/migrate.sh"
  },
  "author": "Mark&Yana",
  "license": "MIT",
  "dependencies": {
    "DatePicker": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "helmet": "^3.21.2",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.9.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.18.2",
    "react-datepicker": "^2.14.0",
    "react-helmet": "^5.2.1",
    "react-scripts": "^3.4.0",
    "react-time-picker": "^3.9.0",
    "validator": "^12.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/komapc/MERN"
  },
  "engines": {
    "node": "13.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
