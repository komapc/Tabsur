{
  "name": "coolanu",
  "version": "1.0.0",
  "description": "Coolanu (table-surfing) alpha",
  "main": "server.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "export NODE_ENV=production && node server.js",
    "server": "export NODE_ENV=production && nodemon server.js",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "export NODE_ENV=production && npm run-script client-install && cd client && react-scripts build"
  },
  "author": "Mark&Yana",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "concurrently": "^5.1.0",
    "cors": "^2.8.5",
    "express": "^4.16.4",
    "is-empty": "^1.2.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.11",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react-scripts": "^3.4.0",
    "validator": "^12.2.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/komapc/MERN"
  },
  "engines": {
    "node": "13.3.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.2"
  }
}
